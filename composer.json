{
    "name": "kallencode/laravel-twitch",
    "description": "A Laravel package to use the Twitch v5 API",
    "keywords": [
        "kallencode",
        "laravel",
        "twitch",
        "api"
    ],
    "homepage": "https://github.com/kallencode/laravel-twitch",
    "license": "MIT",
    "authors": [
        {
            "name": "Roelof Kallenkoot",
            "email": "roelof@kallencode.nl",
            "homepage": "https://kallencode.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "orchestra/testbench" : "3.4.6",
        "mockery/mockery": "^0.9.9"
    },
    "autoload": {
        "psr-4": {
            "Kallencode\\Twitch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kallencode\\Twitch\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
  }
}
